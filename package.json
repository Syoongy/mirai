    "vue-router": "4.0.11",
    "ynetwork": "3.0.5"
  },
  "devDependencies": {
    "@tauri-apps/cli": "1.0.0-beta.10",
    "@vitejs/plugin-vue": "^1.6.1",
    "@vue/compiler-sfc": "^3.2.9",
    "autoprefixer": "^10.3.4",
    "cross-env": "^7.0.3",
    "postcss": "^8.3.6",
    "sass": "1.39.0",
    "sass-loader": "12.1.0",
    "tailwindcss": "^2.2.11",
    "vite": "2.5.4"
