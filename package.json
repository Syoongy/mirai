{
  "name": "vite-tauri-template",
  "version": "0.0.1",
  "description": "Mirai",
  "keywords": [
    "vite",
    "tauri",
    "vue",
    "tailwind",
    "desktop"
  ],
  "scripts": {
    "lint": "exit 0;",
    "serve": "vite --port 8080",
