    "ky": "^0.30.0",
    "vue": "^3.2.33",
    "vue-router": "4.0.14"
  },
  "devDependencies": {
    "@tauri-apps/cli": "1.0.0-rc.8",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/compiler-sfc": "^3.2.33",
    "autoprefixer": "^10.4.4",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.12",
    "tailwindcss": "^3.0.24",
    "vite": "2.9.5"
  },
  "license": "ISC"
