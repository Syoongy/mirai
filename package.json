    "vue": "^3.2.26",
    "vue-router": "4.0.12"
  },
  "devDependencies": {
    "@tauri-apps/cli": "1.0.0-beta.10",
    "@vitejs/plugin-vue": "^2.0.1",
    "@vue/compiler-sfc": "^3.2.26",
    "autoprefixer": "^10.4.1",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.5",
    "tailwindcss": "^3.0.11",
    "vite": "2.7.10"
