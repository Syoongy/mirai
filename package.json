{
  "name": "vite-tauri-template",
  "version": "0.0.1",
  "description": "Mirai",
  "keywords": [
    "vite",
    "tauri",
    "vue",
    "tailwind",
    "desktop"
  ],
  "scripts": {
    "lint": "exit 0;",
    "serve": "vite --port 8080",
    "serve:native": "cross-env VITE_HTTP_HANDLER=NATIVE vite --port 8080",
    "serve:tauri": "tauri dev",
    "build:web": "vite build",
    "build": "cross-env VITE_HTTP_HANDLER=NATIVE vite build && tauri build"
  },
  "dependencies": {
    "@tauri-apps/api": "1.0.0-beta.5",
    "vue": "^3.2.2",
    "vue-router": "4.0.10",
    "ynetwork": "3.0.5"
  },
  "devDependencies": {
    "@tauri-apps/cli": "1.0.0-beta.6",
    "@vitejs/plugin-vue": "^1.4.0",
    "@vue/compiler-sfc": "^3.2.2",
    "cross-env": "^7.0.3",
    "sass": "1.37.5",
    "sass-loader": "12.1.0",
    "vite": "2.4.4"
  },
  "license": "ISC"
}
